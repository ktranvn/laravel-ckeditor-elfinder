{
    "name": "ktran/ckeditor-elfinder",
    "keywords": ["laravel", "ktran", "api"],
    "description": "CKeditor with Elfinder for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin",
            "email": "tranthai@enjoyworks.co.kr"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5|^6|^7|^8",
        "league/flysystem-aws-s3-v3": "^1.0",
        "barryvdh/laravel-elfinder": "^0.4.7",
        "intervention/image": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Ktran\\CE\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ktran\\CE\\CEServiceProvider"
            ],
            "aliases": {
                "CE": "Ktran\\CE"
            }
        }
    },

    "minimum-stability": "dev"
}
